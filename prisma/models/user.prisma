model User {
  id        Int       @id @default(autoincrement())
  email     String    @unique
  name      String?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?

  ownedNotes Note[]           @relation("owned_notes")
  noteAccess UserNoteAccess[]

  @@map("users")
}
